{
  "name": "OpenSprinkler",
  "version": "0.1",
  "slug": "opensprinkler",
  "description": "Run the OpenSprinkler Pi (OSPi) firmware on top of Hass.io",
  "url": "https://github.com/rbhr/hassio-addons/tree/master/OpenSprinkler",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "startup": "services",
  "webui": "http://[HOST]:[PORT:8081]/",
  "boot": "auto",
  "ports": {
    "8080/tcp": 8081,
    "80/tcp": 8082
  },
  "privileged": [
    "SYS_RAWIO"
  ],
  "gpio": "yes",
  "options": {},
  "schema": {}
}
